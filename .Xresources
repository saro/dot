! XST Settings
st.termname:     st-256color
st.shell:        /bin/zsh
! st.font:         Droid Sans Mono Slashed for Powerline:pixelsize=16:antialias=true:autohint=true;
st.font:         Share Tech Mono:pixelsize=12:antialias=true:autohint=true;
st.borderpx:     5

! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape:       2
st.cursorthickness:   2
! 0: normal blinking, 1: leave cursor border and blink with cursors background
st.cursorblinkstyle:  0
! 0: cursor blinks with a constant interval; 1: blinking cycle resets on key input
st.cursorblinkontype: 1
st.bold_font:         1
st.xfps:              120
st.actionfps:         30

! Kerning / character bounding-box height multiplier
st.chscale:           1.0
! Kerning / character bounding-box width multiplier
st.cwscale:           1.0
! blinking timeout for terminal and cursor blinking (0 disables)
st.blinktimeout:      800
! bell volume. Value between -100 and 100. (0 disables)
st.bellvolume:        0
! this is a char that is exposed like so: `printf '\033[z'`
st.prompt_char:       $

! opacity==255 means what terminal will be not transparent, 0 - fully transparent
st.opacity:      255

! Defaults taken from base16-twilight colorscheme.
st.foreground:   #ffffff
st.background:   #000000
st.cursorfg:     #bc6a00
st.cursorbg:     #ffffff

! Thx @fmount
! INFO SECTION CONTAINING ALL COLOR DEFINITIONS !
!! Black + DarkGrey
st.color0:  #000000
st.color8:  #555753

!! DarkRed + Red
st.color1:  #cc0000
st.color9:  #ff6565

!! DarkGreen + Green
st.color2:  #4e9a06
st.color10: #92f429

!! DarkYellow + Yellow
st.color3:  #c4a000
st.color11: #eab93d

!! DarkBlue + Blue
st.color4:  #3465a4
st.color12: #3465a4

!! DarkMagenta + Magenta
st.color5:  #75507b
st.color13: #ce5c00

!!DarkCyan + Cyan (both not tango)
st.color6:  #06989a
st.color14: #00cdcd

!! LightGrey + White
st.color7:  #d3d7cf
st.color15: #ffffff

!! urxvt
URxvt.perl-ext: url-select
URxvt.perl-ext-common:
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.iso14755: false
URxvt.iso14755_52: false
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
URxvt.font: xft:Share Tech Mono:size=10,xft:Symbola
URxvt.color0:  #000000
URxvt.color8:  #555753
!! DarkRed + Red
URxvt.color1:  #cc0000
URxvt.color9:  #ff6565
!! DarkGreen + Green
URxvt.color2:  #4e9a06
URxvt.color10: #92f429
!! DarkYellow + Yellow
URxvt.color3:  #c4a000
URxvt.color11: #eab93d
!! DarkBlue + Blue
URxvt.color4:  #3465a4
URxvt.color12: #3465a4
!! DarkMagenta + Magenta
URxvt.color5:  #75507b
URxvt.color13: #ce5c00
!!DarkCyan + Cyan (both not tango)
URxvt.color6:  #06989a
URxvt.color14: #00cdcd
!! LightGrey + White
URxvt.color7:  #d3d7cf
URxvt.color15: #ffffff
URxvt.foreground: #ffffff
URxvt.background: #000000
URxvt.cursorColor: #bc6a00
URxvt.cursorColor2: #ffffff
URxvt.scrollBar: false
